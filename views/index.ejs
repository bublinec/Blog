<%- include("partials/header") %> 

<h1>Blogs</h1>
<% blogs.forEach(function (blog){ %>
    <h2><%= blog.title %></h2>
    <img src="<%= blog.img_url %>" alt="<%= blog.title + "image" %> ">
    <p><%= blog.body.substring(0, 100) %>...</p>
    <h5><%= blog.date %> </h5>
    <a href="/blogs/<%= blog._id %>">Read More</a>
<% }) %> 

<%- include("partials/footer") %> 
